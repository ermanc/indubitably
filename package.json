{
  "name": "indubitably",
  "version": "0.0.1",
  "description": "Simple, functional JSON data validator",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ermanc/indubitably.git"
  },
  "keywords": [
    "json",
    "schema",
    "validate"
  ],
  "scripts": {
    "build-continuous": "npm install && npm run prepublish",
    "lint": "eslint --ext .js src/*",
    "test": "mocha 'test/**/*_test.js' --compilers js:babel/register",
    "prepublish": "babel src/ -d dist/"
  },
  "dependencies": {
    "lodash": "^3.10.1",
    "moment": "^2.7.0"
  },
  "devDependencies": {
    "babel": "^5.8.19",
    "babel-eslint": "^4.1.3",
    "eslint": "^1.7.0",
    "expect.js": "^0.3.1",
    "mocha": "^2.3.2"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "rules": {
      "strict": 0
    }
  },
  "author": "Erman Celen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ermanc/indubitably/issues"
  },
  "homepage": "https://github.com/ermanc/indubitably#readme"
}
